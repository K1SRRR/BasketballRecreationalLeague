<Window x:Class="BasketballRecreationalLeague.Views.TeamWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BasketballRecreationalLeague.Views"
        mc:Ignorable="d"
        Title="TeamWindow" Height="720" Width="1280">
    <Grid>
        <Label Content="{Binding TeamDTO.Name, UpdateSourceTrigger=PropertyChanged}"/>
        <Label Content="{Binding TeamDTO.YearOfEstablishment, UpdateSourceTrigger=PropertyChanged}"/>
    </Grid>
</Window>
